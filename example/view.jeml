

import layout "./layout" // import default mixin from layout.jeml

default {
    % var title = "I am title";
    `layout(title, body())`
}

mixin body {
    p: "I am body"

    % var users = {
    %     "foo": "I am foo",
    %     "bar": "I am bar"
    % }

    for name: introduction in `users` {
        p: { "name: " `name`}
        p: { "introduce: " `introduction`}
    }
}

export print_name (name) {
    p: { "my name is " `name` }
}
